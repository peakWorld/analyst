    "@babel/types": "^7.23.6",
    "clipanion": "^4.0.0-rc.2",
    "esbuild": "^0.19.9",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.32",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.9",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
